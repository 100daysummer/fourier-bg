{
  "name": "Fourier",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "watch": {
    "makehtml": "content.md",
    "makedebughtml": "debug.md",
    "webpack": "js/**/*.js",
    "cp": "css/styles.css"
  },
  "scripts": {
    "start": "http-server",
    "webpack": "webpack || exit 1",
    "makehtml": "showdown makehtml -i content.md -o /tmp/content.html && sed '/{{CONTENT}}/ {\nr /tmp/content.html\nd\n}' template.html > build/index.html",
    "makedebughtml": "showdown makehtml -i debug.md -o /tmp/debug.html && sed '/{{CONTENT}}/ {\nr /tmp/debug.html\nd\n}' template.html > build/debug.html",
    "cp": "cp -r css build/",
    "build": "npm run webpack && npm run makehtml && npm run cp",
    "test": "jest",
    "watch": "npm-watch",
    "reload": "reload -b -d build"
  },
  "author": "Jezzamon",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "http-server": "^0.11.1",
    "jest": "^23.6.0",
    "npm-watch": "^0.3.0",
    "regenerator-runtime": "^0.12.1",
    "reload": "^2.3.1",
    "webpack": "^3.12.0"
  },
  "dependencies": {
    "fft.js": "^4.0.3",
    "matrix-multiplication": "^0.5.2"
  }
}
